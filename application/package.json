{
  "name": "nuxt-template",
  "version": "1.0.0",
  "description": "My Nuxt Template Project",
  "author": "pt-suzuki",
  "private": true,
  "scripts": {
    "dev": "cross-env NUXT_HOST=0.0.0.0 NODE_ENV=development nuxt-ts",
    "build": "nuxt-ts build",
    "dev-build": "cross-env NODE_ENV=development nuxt-ts build",
    "prod-build": "cross-env NODE_ENV=production nuxt-ts build",
    "dev-deploy": "npm run dev-build && sls deploy --stage dev",
    "staging-deploy": "npm run build && sls deploy --stage staging",
    "start": "cross-env NODE_ENV=production nuxt-ts start",
    "generate": "nuxt-ts generate",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/types": "^0.6.1",
    "@nuxt/typescript-build": "^0.5.6",
    "@nuxt/typescript-runtime": "^0.3.7",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@types/auth0-lock": "^11.4.12",
    "@types/jwt-decode": "^2.2.1",
    "auth0-lock": "^11.20.2",
    "axios": "^0.19.1",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "jwt-decode": "^2.2.0",
    "moment": "^2.24.0",
    "nuxt": "^2.0.0",
    "nuxt-property-decorator": "^2.5.0",
    "query-string": "^6.9.0",
    "vue-property-decorator": "^8.3.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@types/node": "^13.1.6",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.6.0",
    "jest": "^24.1.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.16.4",
    "vue-jest": "^4.0.0-0"
  }
}
